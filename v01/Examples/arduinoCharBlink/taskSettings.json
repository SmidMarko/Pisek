{
"checker": "tests/gen/checker.cpp",
"checkerDeps": [
    {"name": "jsonxx.cc",
     "path": "$ROOT_PATH/_common/jsonxx/jsonxx.cc"},
    {"name": "jsonxx.h",
     "path": "$ROOT_PATH/_common/jsonxx/jsonxx.h"}],
"correctSolutions": [
    {"path": "$TASK_PATH/tests/gen/sol-ok-c.c",
     "grade": 100,
     "language": "cpp"},
    {"path": "$TASK_PATH/tests/gen/sol-bad-c.c",
     "grade": 0,
     "language": "cpp"}
    ],
"defaultEvaluationSolutions": [{
    "id": "@solutionId",
    "compilationDescr": {
        "language": "cpp",
        "files": [
            {"name": "main.c",
             "path": "$ROOT_PATH/_common/arduino/main.c"}],
        "dependencies": [
            {"name": "program.h",
             "path": "@solutionPath",
             "content": "@solutionContent"},
            {"name": "libarduino.h",
             "path": "$ROOT_PATH/_common/arduino/libarduino.h"}]
    },
    "compilationExecution": "@defaultSolutionCompParams"}]
}
